<html>
<head>
    <title>
        {% block title %}
            آموزش آنلاین
        {% endblock %}
    </title>
    {% block meta %}
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <meta name="description" content="">
        <meta name="author" content="">
    {% endblock %}
    {% block stylesheet %}
        <link rel="stylesheet" type="text/css" href="{{ asset("bundles/elearningmain/css/style.css") }}">
        <link rel="stylesheet" type="text/css" href="{{ asset("bundles/elearningmain/css/style-responsive.css") }}">
        <link rel="stylesheet" type="text/css" href="{{ asset("bundles/elearningmain/css/ELearning.css") }}">
    {% endblock %}
    {% block icon %}
        <link rel="shortcut icon" href="{{ asset("bundles/elearningmain/ico/favicon.ico") }}">
        <link rel="apple-touch-icon-precomposed" sizes="144x144" href="{{ asset("bundles/elearningmain/ico/apple-touch-icon-144-precomposed.png") }}">
        <link rel="apple-touch-icon-precomposed" sizes="114x114" href="{{ asset("bundles/elearningmain/ico/apple-touch-icon-114-precomposed.png") }}">
        <link rel="apple-touch-icon-precomposed" sizes="72x72" href="{{ asset("bundles/elearningmain/ico/apple-touch-icon-72-precomposed.png") }}">
        <link rel="apple-touch-icon-precomposed" href="{{ asset("bundles/elearningmain/ico/apple-touch-icon-57-precomposed.png") }}">
    {% endblock %}
    {% block javascript %}
        <script src="{{ asset("bundles/elearningmain/js/jquery.js") }}"></script>
        <script src="{{ asset("bundles/elearningmain/js/jquery.nivo.slider.pack.js") }}"></script>
        <script src="{{ asset("bundles/elearningmain/js/jquery.isotope.min.js") }}"></script>
        <script src="{{ asset("bundles/elearningmain/js/bootstrap-alert.js") }}"></script>
        <script src="{{ asset("bundles/elearningmain/js/formvalidation.js") }}"></script>
        <script src="{{ asset("bundles/elearningmain/js/smoothscroll.js") }}"></script>
        <script src="{{ asset("bundles/elearningmain/js/html5placeholder.jquery.js") }}"></script>
        <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
        <script src="{{ asset("bundles/elearningmain/js/html5shiv.js") }}" type="text/javascript"></script>
        <script src="{{ asset("bundles/elearningmain/js/excanvas.js") }}" type="text/javascript"></script>
        <script src="{{ asset("bundles/elearningmain/js/ie_fix.js") }}" type="text/javascript"></script>
    {% endblock %}
</head>
<body>
{% block Body %}
{% endblock %}
</body>
</html>